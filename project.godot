; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ball_is_coming"
config/features=PackedStringArray("4.2", "Mobile")

[display]

window/size/viewport_width=144
window/size/viewport_height=256
window/size/window_width_override=720
window/size/window_height_override=1280
window/stretch/mode="canvas_items"
window/handheld/orientation=5

[editor_plugins]

enabled=PackedStringArray("res://addons/Asset_Drawer/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "orange",
"res://resources/": "purple",
"res://scenes/": "blue",
"res://scripts/": "green"
}

[rendering]

renderer/rendering_method="mobile"
